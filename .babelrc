{
    "presets": [
        ["env", {
            "modules": false,
            "targets": {
                "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
            }
        }],
        "react",
        "stage-2"
    ],
    "plugins": [
        ["transform-runtime", {
          "helpers": false,
          "polyfill": false,
          "regenerator": true
        }],
        [
            "import",
            {
                "style": "css",
                "libraryName": "antd-mobile"
            }]
        ],
}
